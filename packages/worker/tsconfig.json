{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"lib": ["ESNext"],
		"types": ["@types/node"],
		"strict": true,
		"noEmit": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"isolatedModules": true
	},
	"include": ["src/**/*.ts", "worker-configuration.d.ts"],
	"exclude": ["node_modules"]
}
